{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "api/FBLM.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "./api/FBLM.mjs"
      },
      {
        "src": "/loginregister",
        "dest": "/dist/loginregister.html"
      },
      {
        "src": "/loginregister/(.*)",
        "dest": "/dist/loginregister/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }